{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@elements/*": ["src/elements/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@images/*": ["src/assets/images/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@footer/*": ["src/footerComponents/*"],
      "@header/*": ["src/headerComponents/*"],
      // "@assets/*": ["src/assets/*"]
    }
  }
}
